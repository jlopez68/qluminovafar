<div style="width: 80%; text-align:center; margin-left:10%">

{{#if notes}}
  <table class="table table-bordered table-hover table-primary table-striped-columns">
    <thead>
      <tr>

        <th>GRUPO</th>
        <th>FECHA</th>
        <th>EQUIPO</th>
        <th>RESULTADO</th>
        <th>EQUIPO </th>
        <th>RESULTADO</th>
        <th >Accion</th>
      </tr>
    </thead>
    <tbody>
      {{#each notes}}
        <tr>

          <td>{{grupo}}</td>
          <td>{{fecha}}</td>
          <td>{{equipo1}}</td>
          <td>{{resultado1}}</td>
          <td>{{equipo2}}</td>
          <td>{{resultado2}}</td>
          <td><a href="/notes/edit/{{_id}}"><i class="fas fa-pen-alt" style="color: black"></i></a></td>
          
        </tr>
       {{/each}}
    </tbody>
  </table>
{{/if}}
</div>

