{{#each errors}}
  <div class="alert alert-danger alert-dismissible fade show" role="alert">
    {{text}}
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
{{/each}}
<div class="col-md-4 mx-auto">
  <div class="card">
    <div class="card-header bg-dark text-center">
      <h3 class="text-white">Datos del Medico</h3>
    </div>
    <div class="card-body text-right">
        <form action="/notes/visualizar-notas/{{note._id}}?_method=PUT" method="POST">
        <input type="hidden" name="_method" value="PUT" />
        <div class="mb-3">
          <label for="title">Nombre</label>
          <input
            type="text"
            name="title"
            class="form-control"
            placeholder="Title"
            value="{{note.title}}"
            readonly
          />

        </div>

                <div class="mb-3">
          <label for="description">Apellido</label>
            <input
            type="text"
            name="description"
            class="form-control"
            placeholder="Cantidad"
            value = "{{note.description}}"
               readonly
          />
        </div>
        <div class="mb-3">
          <label for="description">Registro</label>
            <input
            type="text"
            name="registro"
            class="form-control"
            value = "{{note.registro}}"
            readonly
          />
        </div>
        <div class="mb-3">
          <label for="description">Celular</label>
            <input
            type="text"
            name="celular"
            class="form-control"
            value = "{{note.celular}}"
            readonly
          />
        </div>
        <div class="mb-3">
          <label for="description">Email</label>
            <input
            type="text"
            name="email"
            class="form-control"
            value = "{{note.email}}"
            readonly
          />
        </div>
        <div class="mb-3">
          <label for="description">Direccion</label>
            <input
            type="text"
            name="direccion"
            class="form-control"
            value = "{{note.direccion}}"
            readonly
          />
        </div>
        <div class="mb-3">
          <label for="description">calle</label>
            <input
            type="text"
            name="calle"
            class="form-control"
            value = "{{note.calle}}"
            readonly
          />
        </div>
        <div class="mb-3">
          <label for="description">Ciudad o Barrio</label>
            <input
            type="text"
            name="barrio"
            class="form-control"
            value = "{{note.barrio}}"
            readonly
          />

        </div>

	<div class="contenedor">

<canvas width="620" height="360" id="can"></canvas>


	</div>

<script>
var data1 = "{{note.firma}}";
var can = document.getElementById('can');
var ctx = can.getContext('2d');
var img = new Image();
img.src = data1;
//img.src = "data:image/png;base64,";

img.addEventListener('load', e =>
{
  ctx.drawImage(img, 0, 0);
  
  //ctx.fillText('My random text', 100, 100);
  var dataURL = can.toDataURL("image/png");
  var image = new Image();
  var element = document.createElement('a');

  image.src = dataURL;

  element.setAttribute('href', image.src);
  //element.setAttribute('download', 'image');
  
  document.body.appendChild(element);
  
  //element.click();
});



